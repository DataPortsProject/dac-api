{
  "env": {
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["promise", "prettier"],
  "rules": {
    "prettier/prettier": ["warn", {
      "singleQuote": true
    },{"usePrettierrc":false}],
    "no-use-before-define": 0,
    "comma-dangle":["error", {
      "objects": "always-multiline",
      "imports": "only-multiline",
      "exports": "only-multiline",
      "arrays": "only-multiline"
    }],
    "quotes": ["error", "single"]
  }
}
